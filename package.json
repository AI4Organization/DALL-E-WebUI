{
  "name": "dall-e-image-generator",
  "version": "3.0.8",
  "description": "Images generator with DALL-E Models",
  "author": "AI4Organization Engineers",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:rsbuild\"",
    "dev:backend": "nodemon --exec tsx server/index.ts",
    "dev:rsbuild": "rsbuild dev",
    "build": "npm run build:backend && npm run build:rsbuild",
    "build:backend": "tsc --project tsconfig.server.json",
    "build:rsbuild": "rsbuild build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:rsbuild\"",
    "start:backend": "node server/dist/index.js",
    "start:rsbuild": "rsbuild preview"
  },
  "dependencies": {
    "@rsbuild/core": "^1.7.1",
    "@rspack/core": "^1.7.0",
    "@tailwindcss/postcss": "^4.1.18",
    "antd": "^6.1.3",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "framer-motion": "^12.23.26",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "openai": "^6.15.0",
    "react": "19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.4.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.4",
    "@types/react": "19.2.7",
    "autoprefixer": "^10.4.23",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "eslint": "9.39.2",
    "follow-redirects": "^1.15.11",
    "ieee754": "^1.2.1",
    "nanoid": "^5.1.6",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "queue-microtask": "^1.2.3",
    "resolve-pkg-maps": "^1.0.0",
    "run-parallel": "^1.2.0",
    "safe-buffer": "^5.2.1",
    "simple-concat": "^1.0.1",
    "simple-get": "^4.0.1",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "zod": "^4.3.4"
  }
}
